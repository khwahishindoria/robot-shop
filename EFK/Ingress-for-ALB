apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  namespace: logging
  name: kibana-logging-ingress
  annotations:
    kubernetes.io/ingress.class: alb
    alb.ingress.kubernetes.io/scheme: internet-facing
    alb.ingress.kubernetes.io/target-type: ip
spec:
  rules:
    - http:
        paths:
          - path: /
            pathType: Prefix
            backend:
              service:
                name: kibana-logging
                port:
                  number: 5601
